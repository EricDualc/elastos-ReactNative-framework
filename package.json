{
  "name": "elastos-ReactNative-framework",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:test": "npm link && bundle --dev --platform ios --entry-file test.js --bundle-output test.js && npm unlink",
    "build:example": "npm link && bundle --platform ios --dev --entry-file example.js --bundle-output example.js && npm unlink",
    "dep:start": "docker-compose -f ./docker/docker-compose.yml up -d",
    "dep:stop": "docker-compose -f ./docker/docker-compose.yml down"
  },
  "dependencies": {
    "color": "1.0.3",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "native-base": "2.7.0",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-navigation": "1.5.0"
  },
  "devDependencies": {
    "jest": "23.4.2",
    "react-native-bundle": "0.0.4",
    "babel-eslint": "7.2.3",
    "eslint": "4.4.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-react-native": "3.0.1",
    "flow-bin": "0.52.0",
    "flow-typed": "2.1.5",
    "husky": "0.14.3",
    "prettier": "1.5.3",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "bin": {
    "bundle": "./bin/bundle"
  }
}
